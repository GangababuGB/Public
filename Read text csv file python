############# Method 1
import csv
file = open('Unemployment_Rate_M_Total.txt')
type(file)

csvreader = csv.reader(file)

print(csvreader)
header = []
header = next(csvreader)
header = next(csvreader)

rows = []
for row in csvreader:
        rows.append(row)

print(header)
print(rows)

############## Method 2 simple and effective
with open('Unemployment_Rate_M_Total.txt') as file:
    content = file.read()
header = content[1:2]
rows = content[2:]
print(header)
print(rows)
